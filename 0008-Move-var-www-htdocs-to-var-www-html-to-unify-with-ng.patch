From bfe19eabbf59867c7f95fe740c719435a0113eaf Mon Sep 17 00:00:00 2001
From: Auke Kok <auke-jan.h.kok@intel.com>
Date: Wed, 4 Oct 2017 15:09:41 -0700
Subject: [PATCH] Move /var/www/htdocs to /var/www/html to unify with nginx.

---
 config.layout | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/config.layout b/config.layout
index 35a325a..588aec9 100644
--- a/config.layout
+++ b/config.layout
@@ -23,7 +23,7 @@
     installbuilddir: ${datadir}/build
     errordir:      ${datadir}/error
     iconsdir:      ${datadir}/icons
-    htdocsdir:     ${datadir}/htdocs
+    htdocsdir:     ${datadir}/html
     manualdir:     ${datadir}/manual
     cgidir:        ${datadir}/cgi-bin
     includedir:    ${prefix}/include
@@ -48,7 +48,7 @@
     installbuilddir: ${datadir}/build
     errordir:      ${datadir}/error
     iconsdir:      ${datadir}/icons
-    htdocsdir:     ${datadir}/htdocs
+    htdocsdir:     ${datadir}/html
     manualdir:     ${datadir}/manual
     cgidir:        ${datadir}/cgi-bin
     includedir:    ${prefix}/include+
-- 
2.14.2

